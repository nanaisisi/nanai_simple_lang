{
	"name": "nasl-syntax",
	"displayName": "Nanai Simple Lang Syntax",
	"description": "Rust風簡易言語(nanai_simple_lang)用のシンタックスハイライト (Rustベース)",
	"version": "0.0.1",
	"publisher": "nanaisisi",
	"license": "MIT",
	"engines": {
		"vscode": "^1.8.0"
	},
	"scripts": {
		"update-grammar": "node ./build/update-grammar.mjs"
	},
	"categories": ["Programming Languages"],
	"contributes": {
		"languages": [
			{
				"id": "nasl",
				"aliases": ["Nanai Simple Lang", "nanai-simple-lang", "Nasl", "nasl"],
				"extensions": [".nasl"],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "nasl",
				"scopeName": "source.nasl",
				"path": "./syntaxes/nasl.tmLanguage.json"
			}
		]
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/nanaisisi/nanai_simple_lang.git"
	}
}
