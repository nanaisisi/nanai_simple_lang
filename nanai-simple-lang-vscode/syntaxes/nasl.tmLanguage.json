	"patterns": [
		{
			"comment": "boxed slice literal",
			"begin": "(<)(\\[)",
			"beginCaptures": {
				"1": {
					"name": "punctuation.brackets.angle.nasl"
				},
				"2": {
					"name": "punctuation.brackets.square.nasl"
				}
			},
			"end": ">",
			"endCaptures": {
				"0": {
					"name": "punctuation.brackets.angle.nasl"
				}
			},
			"patterns": [
				{
					"include": "#block-comments"
				},
				{
					"include": "#comments"
				},
				{
					"include": "#gtypes"
				},
				{
					"include": "#lvariables"
				},
				{
					"include": "#lifetimes"
				},
				{
					"include": "#punctuation"
				},
				{
					"include": "#types"
				}
			]
		},
		{
			"comment": "macro type metavariables",
			"name": "meta.macro.metavariable.type.nasl",
			"match": "(\\$)((crate)|([A-Z]\\w*))(\\s*(:)\\s*(block|expr(?:_2021)?|ident|item|lifetime|literal|meta|pat(?:_param)?|path|stmt|tt|ty|vis)\\b)?",
			"captures": {
				"1": {
					"name": "keyword.operator.macro.dollar.nasl"
				},
				"3": {
					"name": "keyword.other.crate.nasl"
				},
				"4": {
					"name": "entity.name.type.metavariable.nasl"
				},
				"6": {
					"name": "keyword.operator.key-value.nasl"
				},
				"7": {
					"name": "variable.other.metavariable.specifier.nasl"
				}
			},
			"patterns": [
				{
					"include": "#keywords"
				}
			]
		},
		{
			"comment": "macro metavariables",
			"name": "meta.macro.metavariable.nasl",
			"match": "(\\$)([a-z]\\w*)(\\s*(:)\\s*(block|expr(?:_2021)?|ident|item|lifetime|literal|meta|pat(?:_param)?|path|stmt|tt|ty|vis)\\b)?",
			"captures": {
				"1": {
					"name": "keyword.operator.macro.dollar.nasl"
				},
				"2": {
					"name": "variable.other.metavariable.name.nasl"
				},
				"4": {
					"name": "keyword.operator.key-value.nasl"
				},
				"5": {
					"name": "variable.other.metavariable.specifier.nasl"
				}
			},
			"patterns": [
				{
					"include": "#keywords"
				}
			]
		},
		{
			"comment": "macro rules",
			"name": "meta.macro.rules.nasl",
			"match": "\\b(macro_rules!)\\s+(([a-z0-9_]+)|([A-Z][a-z0-9_]*))\\s+(\\{)",
			"captures": {
				"1": {
					"name": "entity.name.function.macro.rules.nasl"
				},
				"3": {
					"name": "entity.name.function.macro.nasl"
				},
				"4": {
					"name": "entity.name.type.macro.nasl"
				},
				"5": {
					"name": "punctuation.brackets.curly.nasl"
				}
			}
		},
		{
			"comment": "modules",
			"match": "(mod)\\s+((?:r#(?!crate|[Ss]elf|super))?[a-z][A-Za-z0-9_]*)",
			"captures": {
				"1": {
					"name": "storage.type.nasl"
				},
				"2": {
					"name": "entity.name.module.nasl"
				}
			}
		},
		{
			"comment": "external crate imports",
			"name": "meta.import.nasl",
			"begin": "\\b(extern)\\s+(crate)",
			"beginCaptures": {
				"1": {
					"name": "storage.type.nasl"
				},
				"2": {
					"name": "keyword.other.crate.nasl"
				}
			},
			"end": ";",
			"endCaptures": {
				"0": {
					"name": "punctuation.semi.nasl"
				}
			},
