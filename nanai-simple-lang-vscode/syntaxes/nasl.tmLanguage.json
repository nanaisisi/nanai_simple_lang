	"repository": {
		"comments": {
			"patterns": [
				{
					"comment": "documentation comments",
					"name": "comment.line.documentation.nasl",
					"match": "(///).*$",
					"captures": {
						"1": {
							"name": "punctuation.definition.comment.nasl"
						}
					}
				},
				{
					"comment": "line comments",
					"name": "comment.line.double-slash.nasl",
					"match": "(//).*$",
					"captures": {
						"1": {
							"name": "punctuation.definition.comment.nasl"
						}
					}
				}
			]
		},
		"block-comments": {
			"patterns": [
				{
					"comment": "empty block comments",
					"name": "comment.block.nasl",
					"match": "/\\*\\*/"
				},
				{
					"comment": "block documentation comments",
					"name": "comment.block.documentation.nasl",
					"begin": "/\\*\\*",
					"end": "\\*/",
					"patterns": [
						{
							"include": "#block-comments"
						}
					]
				},
				{
					"comment": "block comments",
					"name": "comment.block.nasl",
					"begin": "/\\*(?!\\*)",
					"end": "\\*/",
					"patterns": [
						{
							"include": "#block-comments"
						}
					]
				}
			]
		},
		"constants": {
			"patterns": [
				{
					"comment": "ALL CAPS constants",
					"name": "constant.other.caps.nasl",
					"match": "\\b[A-Z]{2}[A-Z0-9_]*\\b"
				},
				{
					"comment": "constant declarations",
					"match": "\\b(const)\\s+([A-Z][A-Za-z0-9_]*)\\b",
					"captures": {
						"1": {
							"name": "storage.type.nasl"
						},
						"2": {
							"name": "constant.other.caps.nasl"
						}
					}
				},
				{
					"comment": "decimal integers and floats",
					"name": "constant.numeric.decimal.nasl",
					"match": "\\b\\d[\\d_]*(\\.?)[\\d_]*(?:(E|e)([+-]?)([\\d_]+))?(f32|f64|i128|i16|i32|i64|i8|isize|u128|u16|u32|u64|u8|usize)?\\b",
					"captures": {
						"1": {
							"name": "punctuation.separator.dot.decimal.nasl"
						},
						"2": {
							"name": "keyword.operator.exponent.nasl"
						},
						"3": {
							"name": "keyword.operator.exponent.sign.nasl"
						},
						"4": {
							"name": "constant.numeric.decimal.exponent.mantissa.nasl"
						},
						"5": {
							"name": "entity.name.type.numeric.nasl"
						}
					}
				},
				{
					"comment": "hexadecimal integers",
					"name": "constant.numeric.hex.nasl",
					"match": "\\b0x[\\da-fA-F_]+(i128|i16|i32|i64|i8|isize|u128|u16|u32|u64|u8|usize)?\\b",
					"captures": {
						"1": {
							"name": "entity.name.type.numeric.nasl"
						}
					}
				},
				{
					"comment": "octal integers",
					"name": "constant.numeric.oct.nasl",
					"match": "\\b0o[0-7_]+(i128|i16|i32|i64|i8|isize|u128|u16|u32|u64|u8|usize)?\\b",
					"captures": {
						"1": {
							"name": "entity.name.type.numeric.nasl"
						}
					}
				},
				{
					"comment": "binary integers",
					"name": "constant.numeric.bin.nasl",
					"match": "\\b0b[01_]+(i128|i16|i32|i64|i8|isize|u128|u16|u32|u64|u8|usize)?\\b",
					"captures": {
						"1": {
							"name": "entity.name.type.numeric.nasl"
						}
					}
				},
				{
					"comment": "booleans",
					"name": "constant.language.bool.nasl",
					"match": "\\b(true|false)\\b"
				}
			]
		},
		"escapes": {
			"comment": "escapes: ASCII, byte, Unicode, quote, regex",
			"name": "constant.character.escape.nasl",
			"match": "(\\\\)(?:(?:(x[0-7][\\da-fA-F])|(u(\\{)[\\da-fA-F]{4,6}(\\}))|.))",
			"captures": {
				"1": {
					"name": "constant.character.escape.backslash.nasl"
				},
				"2": {
					"name": "constant.character.escape.bit.nasl"
				},
				"3": {
					"name": "constant.character.escape.unicode.nasl"
				},
				"4": {
					"name": "constant.character.escape.unicode.punctuation.nasl"
				},
				"5": {
					"name": "constant.character.escape.unicode.punctuation.nasl"
				}
			}
		},
		"attributes": {
			"comment": "attributes",
			"name": "meta.attribute.nasl",
			"begin": "(#)(\\!?)(\\[)",
			"beginCaptures": {
				"1": {
					"name": "punctuation.definition.attribute.nasl"
				},
				"3": {
					"name": "punctuation.brackets.attribute.nasl"
				}
			},
			"end": "\\]",
			"endCaptures": {
				"0": {
					"name": "punctuation.brackets.attribute.nasl"
				}
			},
			"patterns": [
				{
					"include": "#block-comments"
				},
				{
					"include": "#comments"
				},
				{
					"include": "#keywords"
				},
				{
					"include": "#lifetimes"
				},
				{
					"include": "#punctuation"
				},
				{
					"include": "#strings"
				},
				{
					"include": "#gtypes"
				},
				{
					"include": "#types"
				}
			]
		},
		"functions": {
			"patterns": [
				{
					"comment": "pub as a function",
					"match": "\\b(pub)(\\()",
					"captures": {
						"1": {
							"name": "keyword.other.nasl"
						},
						"2": {
							"name": "punctuation.brackets.round.nasl"
						}
					}
				},
				{
					"comment": "function definition",
					"name": "meta.function.definition.nasl",
					"begin": "\\b(fn)\\s+((?:r#(?!crate|[Ss]elf|super))?[A-Za-z0-9_]+)((\\()|(<))",
					"beginCaptures": {
						"1": {
							"name": "keyword.other.fn.nasl"
						},
						"2": {
							"name": "entity.name.function.nasl"
						},
						"4": {
							"name": "punctuation.brackets.round.nasl"
						},
						"5": {
							"name": "punctuation.brackets.angle.nasl"
						}
					},
					"end": "(\\{)|(;)",
					"endCaptures": {
						"1": {
							"name": "punctuation.brackets.curly.nasl"
						},
						"2": {
							"name": "punctuation.semi.nasl"
						}
					},
					"patterns": [
						{
							"include": "#block-comments"
						},
						{
							"include": "#comments"
						},
						{
							"include": "#keywords"
						},
						{
							"include": "#lvariables"
						},
						{
							"include": "#constants"
						},
						{
							"include": "#gtypes"
						},
						{
